{
  "name": "data-handler-backend",
  "version": "1.0.0",
  "description": "Backend API for handling large datasets with millions of rows",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "postbuild": "npx prisma generate",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "migrate": "npx prisma migrate dev",
    "seed": "ts-node src/scripts/seed.ts",
    "check-data": "ts-node src/scripts/check-data.ts",
    "introspect": "ts-node src/scripts/introspect.ts",
    "generate": "npx prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.11",
    "prisma": "^5.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "api",
    "typescript",
    "prisma",
    "express",
    "postgresql"
  ],
  "author": "",
  "license": "MIT"
}
